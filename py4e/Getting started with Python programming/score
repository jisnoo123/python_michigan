sc= input('Enter score bw 0.0 and 1.0')
sc = float(sc)
if sc>1.0:
    print('Error')
elif sc < 0.0:
    print('Error')
else:
    if sc>=0.9:
        print('A')
    elif sc>=0.8:
        print('B')
    elif sc>=0.7:
        print('C')
    elif sc >=0.6:
        print('D')
    elif sc<0.6:
        print('F')
